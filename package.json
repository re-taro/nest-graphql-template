{
  "name": "nest-graphql-template",
  "version": "1.0.0",
  "description": "Template for personal Nest projects",
  "keywords": [
    "Nest",
    "GraphQL",
    "template"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/re-taro/nest-graphql-template"
  },
  "license": "MIT",
  "author": {
    "name": "Rintaro Itokawa",
    "email": "me@re-taro.dev",
    "url": "https://www.re-taro.dev"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "fmt": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" \"prisma/**/*.ts\"",
    "seed": "prisma db seed",
    "studio": "prisma studio",
    "start": "nest start",
    "start:dev": "prisma generate && nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test,prisma}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./e2e/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/apollo": "^10.0.6",
    "@nestjs/common": "^8.0.0",
    "@nestjs/config": "^1.2.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/graphql": "^10.0.6",
    "@nestjs/platform-fastify": "^8.4.0",
    "@prisma/client": "^3.10.0",
    "apollo-server-fastify": "^3.6.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "graphql": "^16.3.0",
    "prisma": "^3.10.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.0.0",
    "@nestjs/platform-express": "^8.4.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/jest": "27.4.1",
    "@types/node": "16.11.26",
    "@types/supertest": "2.0.11",
    "@typescript-eslint/eslint-plugin": "5.14.0",
    "@typescript-eslint/parser": "5.14.0",
    "apollo-server-express": "^3.6.4",
    "eslint": "8.11.0",
    "eslint-config-re-taro": "1.0.8",
    "fastify": "^3.17.0",
    "jest": "^27.2.5",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5",
    "webpack": "^5.0.0"
  },
  "engines": {
    "node": "16.x"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}
